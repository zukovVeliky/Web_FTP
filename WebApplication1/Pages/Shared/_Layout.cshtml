<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <base href="/" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - WebApplication1</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.0/font/bootstrap-icons.css" />
    <link rel="stylesheet" href="~/lib/file-icon-vectors/file-icon-vectors.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/windows-filemanager.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/code-editor.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/filemanager.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/WebApplication1.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                <a class="navbar-brand" asp-area="" asp-page="/Index">WebApplication1</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Privacy">Privacy</a>
                        </li>
                        @if (User?.Identity?.IsAuthenticated == true)
                        {
                            <li class="nav-item">
                                <a class="nav-link text-dark" asp-area="" asp-page="/Admin">Admin</a>
                            </li>
                        }
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Inline">Inline</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/FileManager">FileManager</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/AvatarFileManager">Avatar</a>
                        </li>
                    </ul>
                    <partial name="_LoginPartial" />
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2026 - WebApplication1 - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/ace/ace.js"></script>
    <script src="~/lib/ace/theme-chrome.js"></script>
    <script src="~/lib/ace/theme-monokai.js"></script>
    <script src="~/lib/ace/mode-text.js"></script>
    <script src="~/lib/ace/mode-html.js"></script>
    <script src="~/lib/ace/mode-css.js"></script>
    <script src="~/lib/ace/mode-javascript.js"></script>
    <script src="~/lib/ace/mode-typescript.js"></script>
    <script src="~/lib/ace/mode-php.js"></script>
    <script src="~/lib/ace/mode-csharp.js"></script>
    <script src="~/lib/ace/mode-c_cpp.js"></script>
    <script src="~/lib/ace/mode-java.js"></script>
    <script src="~/lib/ace/mode-python.js"></script>
    <script src="~/lib/ace/mode-ruby.js"></script>
    <script src="~/lib/ace/mode-golang.js"></script>
    <script src="~/lib/ace/mode-rust.js"></script>
    <script src="~/lib/ace/mode-sql.js"></script>
    <script src="~/lib/ace/mode-json.js"></script>
    <script src="~/lib/ace/mode-xml.js"></script>
    <script src="~/lib/ace/mode-yaml.js"></script>
    <script src="~/lib/ace/mode-markdown.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>

